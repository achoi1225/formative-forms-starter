extends layout.pug

block content
    h2 Existing Users

    table(class="table")
        thead
            tr
                th(scope="col") id
                th(scope="col") First Name
                th(scope="col") Last Name
                th(scope="col") Email

        tbody
            tr
                each user in users
                    td #{user.id}
                    td #{user.firstName}
                    td #{user.lastName}
                    td #{user.email}